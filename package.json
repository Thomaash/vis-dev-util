{
  "name": "vis-dev-utils",
  "version": "0.0.0-no-version",
  "description": "Development utilities for the Vis family projects.",
  "homepage": "http://visjs.org/",
  "license": "(Apache-2.0 OR MIT)",
  "repository": {
    "type": "git",
    "url": "https://github.com/visjs/vis-dev-utils.git"
  },
  "bugs": {
    "url": "https://github.com/visjs/vis-dev-utils/issues"
  },
  "keywords": [
    "development",
    "utils",
    "vis"
  ],
  "scripts": {
    "build": "rollup --config ./rollup.config.js",
    "clean": "rimraf \"./{.nyc_output,.rpt2_cache,bin,coverage,gen}/*\"",
    "lint": "eslint --ext .js,.ts . && prettier --check \"**/*.{css,html,js,json,ts}\"",
    "lint-fix": "eslint --fix --ext .js,.ts . && prettier --write \"**/*.{css,html,js,json,ts}\"",
    "postinstall": "opencollective postinstall || exit 0",
    "prepublishOnly": "npm run build",
    "test": "npm run build && mocha --exit",
    "watch": "rollup --watch --config rollup.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "bin": {
    "generate-examples-index": "./bin/generate-examples-index.js"
  },
  "files": [
    "LICENSE*",
    "README.md",
    "bin"
  ],
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "globby": "^10.0.1",
    "jimp": "^0.8.5",
    "pageres": "^5.2.0",
    "prettier": "^1.18.2",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/register": "^7.6.2",
    "@types/chai": "^4.2.3",
    "@types/cheerio": "^0.22.13",
    "@types/jimp": "^0.2.28",
    "@types/mocha": "^5.2.7",
    "@types/prettier": "^1.18.3",
    "@types/sinon": "^7.5.0",
    "@types/yargs": "^13.0.3",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "builtin-modules": "^3.1.0",
    "chai": "^4.2.0",
    "core-js": "^3.2.1",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.1",
    "mocha": "^6.2.1",
    "opencollective": "^1.0.3",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.23.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "sinon": "^7.5.0",
    "tmp-promise": "^2.0.2",
    "typescript": "^3.6.3"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/visjs"
  }
}
